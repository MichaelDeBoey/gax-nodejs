{
  "name": "showcase-echo-client",
  "version": "0.1.0",
  "description": "Showcase client for Node.js",
  "repository": "googleapis/nodejs-showcase",
  "license": "Apache-2.0",
  "author": "Google LLC",
  "files": [
    "build/src"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google showcase",
    "showcase",
    "echo",
    "sequence service"
  ],
  "scripts": {
    "clean": "rm -rf build",
    "compile": "tsc -p ./tsconfig.json && minifyProtoJson",
    "compile-protos": "compileProtos src",
    "prefetch": "rm -rf node_modules package-lock.json google-gax*.tgz gapic-tools*.tgz && cd ../../.. && npm pack && mv google-gax*.tgz esm/test/showcase-echo-client/google-gax.tgz && cd ../tools && npm install && npm pack && mv gapic-tools*.tgz ../gax/esm/test/showcase-echo-client/gapic-tools.tgz",
    "prepare": "npm run clean && npm run compile-protos && npm run compile "
  },
  "dependencies": {
    "google-gax": "./google-gax.tgz"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@types/node": "^20.11.26",
    "babel-plugin-replace-import-extension": "^1.1.4",
    "gapic-tools": "./gapic-tools.tgz",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=v18"
  }
}
